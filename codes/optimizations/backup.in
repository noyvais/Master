spinsys {
    nuclei 1H
    channels 1H
} 

par {
    start_operator   I1z
    detect_operator  I1x
    spin_rate        10000
    sw               10000
    crystal_file     rep100
    np               1
    proton_frequency 400e6
    gamma_angles 10
    variable rf      50000
    variable T1 0.25e6/rf
}

proc pulseq {} {
    global par
    pulse $par(T1) $par(rf) y
    acq
}

proc main {} {
    global par
    set f [fsimpson]
    fsave $f $par(name).fid -xreim
}